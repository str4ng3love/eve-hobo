
datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

generator client {
    provider             = "prisma-client-py"
    interface            = "asyncio"
    recursive_type_depth = 5
}

model Item {
    id     String    @id  @map("_id") @default(uuid())
    typeId Int
    name   String
}
